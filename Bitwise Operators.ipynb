    "#PROGRAM TO DEMONSTRATE THE USE OF BITWISE OPERATORS\n",
    "a=10 #00001010\n",
    "b=4  #00000100\n",
    "print(\"AND\",a&b)\n",
    "print(\"OR\",a|b)\n",
    "print(\"NOT\",~b)\n",
    "print(\"XOR\",a^b)\n",
    "print(\"RIGHT SHIFT\",a>>2)\n",
    "print(\"LEFT SHIFT\",a<<2)"
   ]
  }
